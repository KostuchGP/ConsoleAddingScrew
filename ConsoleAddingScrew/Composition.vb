    Sub searchForScrew(ByVal thread As String, ByVal lenghtScrew As Double)
        For i = 0 To V_ROND_HM.Length
            If V_ROND_HM(i, 1) = thread Then
                If V_ROND_HM(i, 2) <= lenghtScrew Then
                    plik = V_ROND_HM(i, 0)
                    Exit For
                End If
            End If
        Next
    End Sub

    Sub searchForDowel(ByVal diameter As String, ByVal lenghtDowel As Double)
        For i = 0 To G_GT.Length
            If G_GT(i, 1) = diameter Then
                If G_GT(i, 2) <= lenghtDowel Then
                    plik = G_GT(i, 0)
                    Exit For
                End If
            End If
        Next
    End Sub

